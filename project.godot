Letter="*res://scenes/letter.tscn"
Arrays="*res://scripts/arrays.gd"
CsvExporter="*res://scripts/csv_exporter.gd"
SoundManager="*res://scripts/SoundManager.gd"
ConfigPlayer="*res://scenes/config_player.tscn"

[display]

